{
  "name": "FRPC Client",
  "version": "1.0.0",
  "slug": "frpc",
  "description": "FRP Client (frpc) add-on for Home Assistant. Establish reverse proxy connections to FRP servers.",
  "url": "https://github.com/Acmen0102/linknlink-remote",
  "startup": "services",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "options": {
    "server_addr": "",
    "server_port": 7000,
    "token": "",
    "local_ip": "127.0.0.1",
    "local_port": 22,
    "remote_port": 6000,
    "proxy_name": "homeassistant_proxy",
    "protocol": "tcp",
    "log_level": "info",
    "log_max_days": 3
  },
  "schema": {
    "server_addr": "str",
    "server_port": "int(0,65535)",
    "token": "str?",
    "local_ip": "str",
    "local_port": "int(1,65535)",
    "remote_port": "int(1,65535)",
    "proxy_name": "str",
    "protocol": "match(tcp|udp|http|https|stcp|sudp|xtcp)",
    "log_level": "match(trace|debug|info|warn|error)",
    "log_max_days": "int(1,30)"
  },
  "ports": {},
  "ports_description": {}
}
